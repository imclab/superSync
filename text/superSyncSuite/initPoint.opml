on initPoint (name) {	//Changes		//11/24/13; 12:03:33 PM by DW			//Created.	local (adrdata = superSyncSuite.init (), adrpoint = @adrdata^.points.[name]);	if not defined (adrpoint^) {		new (tabletype, adrpoint)};	bundle { //prefs		if not defined (adrpoint^.prefs) {			new (tabletype, @adrpoint^.prefs)};		if not defined (adrpoint^.prefs.enabled) {			adrpoint^.prefs.enabled = false};		if not defined (adrpoint^.prefs.folder) {			adrpoint^.prefs.folder = ""};		if not defined (adrpoint^.prefs.s3path) {			adrpoint^.prefs.s3path = ""}};	bundle { //stats		if not defined (adrpoint^.stats) {			new (tabletype, @adrpoint^.stats)};		if not defined (adrpoint^.stats.ctChecks) {			adrpoint^.stats.ctChecks = 0};		if not defined (adrpoint^.stats.ctUploads) {			adrpoint^.stats.ctUploads = 0};		if not defined (adrpoint^.stats.ctConsecutiveErrors) {			adrpoint^.stats.ctConsecutiveErrors = 0};		if not defined (adrpoint^.stats.ctErrors) {			adrpoint^.stats.ctErrors = 0};		if not defined (adrpoint^.stats.files) {			new (tabletype, @adrpoint^.stats.files)}};	return (adrpoint)};bundle { //test code	initPoint ("fargoDocs")}