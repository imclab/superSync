on simpleScript () {	//Changes		//11/24/13; 12:27:06 PM by DW			//Created.	local (folder = user.prefs.dropboxfolder + "apps:Fargo:html:fargoDocs:", s3folderpath = "/fargo.io/docs/");	local (adroutline = @scratchpad.fileUploadList, dir = right);	bundle { //set up outline		if not defined (adroutline^) {			new (outlinetype, adroutline);			target.set (adroutline);			window.setsize (adroutline, 500, 300);			op.setlinetext (clock.now ())}		else {			target.set (adroutline);			op.fullcollapse ();			op.firstsummit ();			op.insert (clock.now (), up)};		edit (adroutline)};	fileloop (f in folder, infinity) {		if f endswith ".html" {			local (filepath = string.delete (f, 1, sizeof (folder)));			filepath = s3folderpath + string.replaceall (filepath, file.getpathchar (), "/");			s3.newobject (filepath, string (file.readwholefile (f)));			op.insert (filepath, dir); dir = down;			new (tabletype, @atts);			atts.type = "link";			atts.url = "http:/" + filepath;			op.attributes.addgroup (@atts)}};	op.firstsummit ()};	//window.zoom (adroutline)bundle { //test code	simpleScript ()}